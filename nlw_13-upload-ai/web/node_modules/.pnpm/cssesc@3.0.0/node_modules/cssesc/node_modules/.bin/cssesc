#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/edimilsonbraz/Documents/www/PORTFOLIO/rocketseat/all-nlw-rocketseat/nlw_13-upload-ai-web/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/bin/node_modules:/home/edimilsonbraz/Documents/www/PORTFOLIO/rocketseat/all-nlw-rocketseat/nlw_13-upload-ai-web/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/node_modules:/home/edimilsonbraz/Documents/www/PORTFOLIO/rocketseat/all-nlw-rocketseat/nlw_13-upload-ai-web/node_modules/.pnpm/cssesc@3.0.0/node_modules:/home/edimilsonbraz/Documents/www/PORTFOLIO/rocketseat/all-nlw-rocketseat/nlw_13-upload-ai-web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/edimilsonbraz/Documents/www/PORTFOLIO/rocketseat/all-nlw-rocketseat/nlw_13-upload-ai-web/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/bin/node_modules:/home/edimilsonbraz/Documents/www/PORTFOLIO/rocketseat/all-nlw-rocketseat/nlw_13-upload-ai-web/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/node_modules:/home/edimilsonbraz/Documents/www/PORTFOLIO/rocketseat/all-nlw-rocketseat/nlw_13-upload-ai-web/node_modules/.pnpm/cssesc@3.0.0/node_modules:/home/edimilsonbraz/Documents/www/PORTFOLIO/rocketseat/all-nlw-rocketseat/nlw_13-upload-ai-web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cssesc" "$@"
else
  exec node  "$basedir/../../bin/cssesc" "$@"
fi
