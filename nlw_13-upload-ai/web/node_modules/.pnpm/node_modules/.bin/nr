#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/edimilsonbraz/Documents/www/PORTFOLIO/rocketseat/all-nlw-rocketseat/nlw_13-upload-ai/web/node_modules/.pnpm/@antfu+ni@0.21.8/node_modules/@antfu/ni/bin/node_modules:/home/edimilsonbraz/Documents/www/PORTFOLIO/rocketseat/all-nlw-rocketseat/nlw_13-upload-ai/web/node_modules/.pnpm/@antfu+ni@0.21.8/node_modules/@antfu/ni/node_modules:/home/edimilsonbraz/Documents/www/PORTFOLIO/rocketseat/all-nlw-rocketseat/nlw_13-upload-ai/web/node_modules/.pnpm/@antfu+ni@0.21.8/node_modules/@antfu/node_modules:/home/edimilsonbraz/Documents/www/PORTFOLIO/rocketseat/all-nlw-rocketseat/nlw_13-upload-ai/web/node_modules/.pnpm/@antfu+ni@0.21.8/node_modules:/home/edimilsonbraz/Documents/www/PORTFOLIO/rocketseat/all-nlw-rocketseat/nlw_13-upload-ai/web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/edimilsonbraz/Documents/www/PORTFOLIO/rocketseat/all-nlw-rocketseat/nlw_13-upload-ai/web/node_modules/.pnpm/@antfu+ni@0.21.8/node_modules/@antfu/ni/bin/node_modules:/home/edimilsonbraz/Documents/www/PORTFOLIO/rocketseat/all-nlw-rocketseat/nlw_13-upload-ai/web/node_modules/.pnpm/@antfu+ni@0.21.8/node_modules/@antfu/ni/node_modules:/home/edimilsonbraz/Documents/www/PORTFOLIO/rocketseat/all-nlw-rocketseat/nlw_13-upload-ai/web/node_modules/.pnpm/@antfu+ni@0.21.8/node_modules/@antfu/node_modules:/home/edimilsonbraz/Documents/www/PORTFOLIO/rocketseat/all-nlw-rocketseat/nlw_13-upload-ai/web/node_modules/.pnpm/@antfu+ni@0.21.8/node_modules:/home/edimilsonbraz/Documents/www/PORTFOLIO/rocketseat/all-nlw-rocketseat/nlw_13-upload-ai/web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@antfu/ni/bin/nr.mjs" "$@"
else
  exec node  "$basedir/../@antfu/ni/bin/nr.mjs" "$@"
fi
